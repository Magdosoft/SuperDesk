# Query Max and Min Ids for a day

db.getCollection('ingest').aggregate([
        { "$match":{ $and:  
            [
            {"type": { "$eq": "picture"}},            
            {"firstcreated":{"$gte":new Date("2019-09-10")}},
            {"firstcreated":{"$lte":new Date("2019-09-10")}}
            ]
            }
        }
        ,
    { 
         
      $count: "$unique_id"
    
    }
])
